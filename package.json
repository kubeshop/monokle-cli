  "homepage": "https://github.com/kubeshop/monokle-core#readme",
  "devDependencies": {
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.18",
    "@types/yargs": "17.0.12",
    "rimraf": "3.0.2",
    "typescript": "4.8.3"
  },
  "dependencies": {
